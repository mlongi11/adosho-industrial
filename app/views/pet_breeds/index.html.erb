<p id="notice"><%= notice %></p>

<h1>Pet Breeds</h1>

<table>
  <thead>
    <tr>
      <th>Breed</th>
      <th>Pet</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pet_breeds.each do |pet_breed| %>
      <tr>
        <td><%= pet_breed.breed_id %></td>
        <td><%= pet_breed.pet_id %></td>
        <td><%= link_to 'Show', pet_breed %></td>
        <td><%= link_to 'Edit', edit_pet_breed_path(pet_breed) %></td>
        <td><%= link_to 'Destroy', pet_breed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pet Breed', new_pet_breed_path %>
